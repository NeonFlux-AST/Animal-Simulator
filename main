local isHitting = false

local Toggle = FarmTab:CreateToggle({
	Name = "Farming/NewLightningball Grind 5k/Spawn",
	Description = nil,
	CurrentValue = false,
    	Callback = function(Value)
isHitting = Value

if isHitting then
	task.spawn(function()
		while isHitting do
			local player = game:GetService("Players").LocalPlayer
			local char = player.Character
			local hrp = char and char:FindFirstChild("HumanoidRootPart")

			local level = player:FindFirstChild("leaderstats")
				and player.leaderstats:FindFirstChild("Level")
				and player.leaderstats.Level.Value

			-- S√©lection du dummy en fonction du niveau
			local dummy, dummyHRP, dummyHum

			if level and level < 5000 then
				local folder = workspace:FindFirstChild("MAP") and workspace.MAP:FindFirstChild("dummies")
				dummy = folder and folder:FindFirstChild("Dummy")
			else
				local folder = workspace:FindFirstChild("MAP") and workspace.MAP:FindFirstChild("5k_dummies")
				dummy = folder and folder:FindFirstChild("Dummy2")
			end

			if dummy then
				dummyHRP = dummy:FindFirstChild("HumanoidRootPart")
				dummyHum = dummy:FindFirstChild("Humanoid")
			end

			-- T√©l√©portation + attaques
			if dummyHRP and hrp then
				-- T√©l√©portation derri√®re le dummy
				hrp.CFrame = dummyHRP.CFrame + Vector3.new(0, 0, 5)

				-- ‚ö° LightningBall (premier script)
				local lightningArgs = {
					dummyHRP.Position,
					"NewLightningball"
				}
				game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(lightningArgs))
			end

			-- Attaque "2" (deuxi√®me script)
			if dummyHum then
				local args = {
					dummyHum,
					2
				}
				game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(unpack(args))
			end

			task.wait()
		end
	end)
end
    	end
}, "Toggle") -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps


