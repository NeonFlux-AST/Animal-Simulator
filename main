local ToggleActive = false

local BossOrder = {
    "BOSSBEAR",
    "BOSSDEER",
    "BOSSDINO",
    "CENTAUR",
    "CRABBOSS",
    "Griffin",
    "LavaGorilla",
    "DragonGiraffe"
}

local dmgEvent = game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli

-- üìå Label qui montre quel boss est en train d'√™tre tu√©
local BossStatusLabel = FarmTab:CreateLabel({
    Text = "Status : Idle",
    Style = 1
})

local Toggle = FarmTab:CreateToggle({
    Name = "Farming No TP Boss",
    CurrentValue = false,
    Callback = function(Value)
        ToggleActive = Value

        if not Value then
            BossStatusLabel:Set("Status : Idle")
            return
        end
        
        task.spawn(function()
            while ToggleActive do
                
                for _, bossName in ipairs(BossOrder) do
                    
                    if not ToggleActive then break end

                    BossStatusLabel:Set("Status : Killing " .. bossName)

                    local boss = workspace.NPC:FindFirstChild(bossName)

                    -- attendre spawn
                    while ToggleActive and (not boss or not boss:FindFirstChild("Humanoid")) do
                        boss = workspace.NPC:FindFirstChild(bossName)
                        task.wait(0.2)
                    end
                    if not ToggleActive then break end

                    local humanoid = boss:FindFirstChild("Humanoid")
                    if humanoid then
                        while ToggleActive and humanoid.Health > 0 do
                            dmgEvent:FireServer(humanoid, 1)
                            task.wait()
                        end
                    end

                    task.wait(0.3)
                end

                BossStatusLabel:Set("Status : Looping...")
            end

            BossStatusLabel:Set("Status : Idle")
        end)
    end,
})


local PvPTab = Window:CreateTab({
	Name = "PvP",
	Icon = "view_in_ar",
	ImageSource = "Material",
	ShowTitle = true -- This will determine whether the big header text in the tab will show
})


local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- ===========================================
-- üîÑ LISTE DES JOUEURS (DROPDOWN D√âJ√Ä FINI)
-- ===========================================
local function GetPlayersList()
    local list = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        table.insert(list, plr.Name)
    end
    return list
end

local SelectedPlayer = nil

local Dropdown = PvPTab:CreateDropdown({
    Name = "Liste des joueurs",
    Options = GetPlayersList(),
    CurrentOption = {},
    MultipleOptions = false,
    Callback = function(option)
        SelectedPlayer = option
    end
}, "Dropdown")

Players.PlayerAdded:Connect(function()
    Dropdown:SetOptions(GetPlayersList())
end)
Players.PlayerRemoving:Connect(function()
    Dropdown:SetOptions(GetPlayersList())
end)

-- ===========================================
-- üî• VARIABLES ORBIT
-- ===========================================
local isOrbitActive = false
local orbitDistance = 15
local orbitSpeed = 20
local angle = 0

-- Tes sliders si tu les veux dans ce tab aussi :
local DistanceSlider = PvPTab:CreateSlider({
    Name = "Distance Orbit",
    Range = {3, 50},
    Increment = 1,
    CurrentValue = 15,
    Callback = function(Value)
        orbitDistance = Value
    end
})

local SpeedSlider = PvPTab:CreateSlider({
    Name = "Orbit Speed",
    Range = {1, 50},
    Increment = 1,
    CurrentValue = 30,
    Callback = function(Value)
        orbitSpeed = Value
    end
})

-- ===========================================
-- üöÄ TOGGLE TP + ORBIT FARMKILL
-- ===========================================
local Toggle = PvPTab:CreateToggle({
    Name = "TP + FarmKill Orbit",
    CurrentValue = false,
    Callback = function(Value)
        isOrbitActive = Value
        
        -- ACTIVATION
        if Value then

            -- ‚ö†Ô∏è Aucun joueur s√©lectionn√©
            if not SelectedPlayer then 
                warn("S√©lectionne d'abord un joueur dans le dropdown !")
                return
            end

            -- Trouver le joueur cibl√©
            local targetPlayer = Players:FindFirstChild(SelectedPlayer)
            if not targetPlayer 
            or not targetPlayer.Character 
            or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                warn("Impossible de trouver le joueur s√©lectionn√©.")
                return
            end

            -- ==================================
            -- üåÄ TP SUR LE JOUEUR CHOISI
            -- ==================================
            LocalPlayer.Character.HumanoidRootPart.CFrame = 
                targetPlayer.Character.HumanoidRootPart.CFrame

            task.wait(1)

            -- ==================================
            -- üåÄ LANCEMENT DE L‚ÄôORBIT
            -- ==================================
            task.spawn(function()
                while isOrbitActive do
                    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    local target = targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")

                    if not hrp or not target then
                        task.wait(0.1)
                        continue
                    end

                    -- CALCUL ORBIT
                    angle += orbitSpeed / 100
                    local x = math.cos(angle) * orbitDistance
                    local z = math.sin(angle) * orbitDistance

                    hrp.CFrame = CFrame.new(
                        target.Position + Vector3.new(x, 3, z),
                        target.Position
                    )

                    -- ACTION 1 : Hit
                    local args1 = { targetPlayer.Character.Humanoid, 2 }
                    game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(unpack(args1))

                    -- ACTION 2 : Lightningball
                    local args2 = { target.Position, "NewLightningball" }
                    game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(args2))

                    task.wait()
                end
            end)

        else
            -- =======================
            -- ‚õî D√âSACTIV√â ‚Üí STOP ORBIT
            -- =======================
            isOrbitActive = false
        end
    end
})


local isHitting = false -- Variable pour suivre l'√©tat du farm


local Toggle = PvPTab:CreateToggle({
    Name = "Farm Kill/Aura",
    CurrentValue = false,
    Flag = "FarmKillAura", -- identifiant unique
    Callback = function(Value)
        isHitting = Value -- Met √† jour la variable locale

        if isHitting then
            task.spawn(function()
                while isHitting do
                    local localPlayer = game.Players.LocalPlayer
                    if not localPlayer.Character or not localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        task.wait(0.5)
                        continue
                    end

                    -- Trouver le joueur le plus proche
                    local closestPlayer = nil
                    local shortestDistance = math.huge
                    for _, player in pairs(game.Players:GetPlayers()) do
                        if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
                            local dist = (player.Character.HumanoidRootPart.Position - localPlayer.Character.HumanoidRootPart.Position).Magnitude
                            if dist < shortestDistance then
                                shortestDistance = dist
                                closestPlayer = player
                            end
                        end
                    end

                    if closestPlayer and closestPlayer.Character and closestPlayer.Character:FindFirstChild("Humanoid") and closestPlayer.Character.Humanoid.Health > 0 then
                        local targetHRP = closestPlayer.Character.HumanoidRootPart

                        -- Action 1 : D√©g√¢ts classiques
                        local args1 = { closestPlayer.Character.Humanoid, 2 }
                        game:GetService("ReplicatedStorage"):WaitForChild("jdskhfsIIIllliiIIIdchgdIiIIIlIlIli"):FireServer(unpack(args1))

                        -- Action 2 : NewLightningball
                        local args2 = { targetHRP.Position, "NewLightningball" }
                        game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(args2))
                    end

                    task.wait(0)
                end
            end)
        end
    end,
})


